  "scripts": {
    "test": "karma start ClientApp/test/karma.conf.js",
    "clean-build": "rimraf ./build",
    "copyfiles": "cd src && copyfiles **/* -e **/*.ts ../build",
    "prebuild": "npm run clean-build && npm run copyfiles",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run start\"",
